package fernuni.propra.algorithm;

public abstract class AbstractAlgorithmFactory {
	static IAbstractAlgorithmFactory getAlgorithmFactory() {
		return new AbstractAlgorihtmFactory();
	}
	ICandidateSearcher createCandidateSearcher();
	IPositionOptimizer createPositionOptimizer();
	IIlluminationTester createIlluminiationTester();

}
