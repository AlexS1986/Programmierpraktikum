package fernuni.propra.algorithm;

import fernuni.propra.internal_data_model.IRoom;

public abstract class ValidateKAbstract {
	
	boolean validate(IRoom room, IRuntimeIlluminationTester runtime) {
		generateIlluminationTester().testIfRoomIsIlluminated(room, runtime);
	}
	
	protected abstract IIlluminationTester generateIlluminationTester();

}
