package fernuni.propra.internal_data_model;

import java.util.HashSet;

public class Lamp extends Point{
	private boolean on;
	HashSet<Integer> tags = new HashSet<Integer>();
	
	public Lamp(double x, double y) {
		super(x, y);
	}
	
	public Lamp(double x, double y, int tag) {
		super(x, y);
		tags.add(tag);
	}
	
	public Lamp(double x, double y, HashSet<Integer> tags) {
		super(x,y);
		if (tags != null) {
			this.tags = tags;
		}
	}
	
	public void addTag(Integer tag) {
		tags.add(tag);
	}
	
	public HashSet<Integer> getTags() { 
		HashSet<Integer> outSet = new HashSet<Integer>();
		for (Integer integer: )
	}

	public void turnOn() {
		on = true;
	}
	
	public void turnOff() {
		on = false;
	}
	
	public boolean getOn() {
		return on;
	}
	
	
}
